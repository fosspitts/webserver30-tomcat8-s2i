{
  "apiVersion" : "v1",
  "kind" : "Template",
  "labels" : { },
  "metadata" : {
    "annotations" : {
      "description" : "# Base template from which you can deploy and run wars on...",
    },
    "labels" : { },
    "name" : "war-deployer-template"
  },
  "objects" : [ {
    "apiVersion" : "v1",
    "kind" : "ImageStream",
    "metadata" : {
      "annotations" : { },
      "labels" : {
        "app" : "${IMAGESTREAM_OUTPUT}",
        "project" : "${IMAGESTREAM_OUTPUT}",
        "version" : "1.0.0",
        "group" : "tomcat"
      },
      "name" : "${IMAGESTREAM_OUTPUT}"
    },
    "spec" : {
      "dockerImageRepository" : "",
      "tags" : [ ]
    }
  }, {
    "apiVersion" : "v1",
    "kind" : "BuildConfig",
    "metadata" : {
      "annotations" : { },
      "labels" : {
        "app" : "${IMAGESTREAM_OUTPUT}",
        "project" : "${IMAGESTREAM_OUTPUT}",
        "version" : "1.0.0",
        "group" : "tomcat"
      },
      "name" : "${IMAGESTREAM_OUTPUT}-bc"
    },
    "spec" : {
      "output" : {
        "to" : {
          "kind" : "ImageStreamTag",
          "name" : "${IMAGESTREAM_OUTPUT}:latest"
        }
      },
      "source" : {
        "git" : {
          "uri" : "https://github.com/welshstew/webserver30-tomcat8-s2i"
        },
        "images" : [ ],
        "secrets" : [ ],
        "type" : "Git"
      },
      "strategy" : {
        "sourceStrategy" : {
          "env" : [ 
            {
              "name" : "ARTIFACT_GROUPID",
              "value" : "${ARTIFACT_GROUPID}"
            },
            {
              "name" : "ARTIFACT_ARTIFACTID",
              "value" : "${ARTIFACT_ARTIFACTID}"
            },
            {
              "name" : "ARTIFACT_VERSION",
              "value" : "${ARTIFACT_VERSION}"
            },
            {
              "name" : "ARTIFACT_PACKAGING",
              "value" : "${ARTIFACT_PACKAGING}"
            }
        ],
          "from" : {
            "kind" : "ImageStreamTag",
            "name" : "jboss-webserver30-tomcat8-openshift:latest",
            "namespace" : "openshift"
          }
        },
        "type" : "Source"
      },
      "triggers" : [ {
        "imageChange" : {
          "from" : {
            "kind" : "ImageStreamTag",
            "name" : "jboss-webserver30-tomcat8-openshift:latest",
            "namespace" : "openshift"
          }
        },
        "type" : "ImageChange"
      } ]
    }
  } ],
  "parameters" : [ {
    "name" : "ARTIFACT_GROUPID",
    "value" : "com.example"
  }, {
    "name" : "ARTIFACT_ARTIFACTID",
    "value" : "ocp-sample-war"
  }, {
    "name" : "ARTIFACT_VERSION",
    "value" : "1.0.0"
  }, {
    "name" : "ARTIFACT_PACKAGING",
    "value" : "war"
  }, {
    "name" : "IMAGESTREAM_OUTPUT",
    "value" : "simple-war-example"
  } ]
}